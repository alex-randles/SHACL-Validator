@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/> .
@base <http://example.com/base/> .



<#TriplesMap1> a rr:TriplesMap;

rml:logicalSource [ rml:source "MetEireannStationDetails.csv";rml:referenceFormulation ql:CSV];

rr:subjectMap [
rr:template "http://data.example.org/MetEireann/{StationNumber}";

];

rr:predicateObjectMap [
rr:predicate geo:hasGeometry;
rr:objectMap [
rr:template "http://data.example.org/MetEireann/{StationNumber}/{Longitude}_{Latitude}";
rr:class geo:Geometry;
rr:termType rr:IRI;
];
];

rr:predicateObjectMap [
rr:predicate ex:Height;
rr:objectMap  [
rml:reference "Height";
rr:termType rr:Literal;
rr:datatype xsd:float;
];
];

rr:predicateObjectMap [
rr:predicate ex:hasStationName;
rr:objectMap  [
rml:reference "StationName";
rr:termType rr:Literal;
];
];


rr:predicateObjectMap [
rr:predicate ex:hasCountyName;
rr:objectMap [
rml:reference "County";
rr:termType rr:Literal;
];
];


rr:predicateObjectMap [
rr:predicate ex:startYear;
rr:objectMap [
rml:reference "OpenYear";
rr:termType rr:Literal;
rr:datatype xsd:float;
]
];

rr:predicateObjectMap [
rr:predicate ex:closeYear;
rr:objectMap [
rml:reference "CloseYear";
rr:termType rr:Literal;
rr:datatype xsd:float;
];
].



<#MapMetEireannGeo> a rr:TriplesMap;

rml:logicalSource [ rml:source "MetEireannStationDetails.csv"; rml:referenceFormulation ql:CSV];

rr:subjectMap [
rr:template "http://data.example.org/MetEireann/{StationNumber}/{Longitude}_{Latitude}";
rr:class geo:Geometry;
rr:termType rr:IRI;
];

rr:predicateObjectMap [
rr:predicate geo:asWKT;
rr:objectMap [
rr:template "POINT({Longitude} {Latitude})";
rr:termType rr:Literal;
rr:datatype geo:wktLiteral;
];
].


